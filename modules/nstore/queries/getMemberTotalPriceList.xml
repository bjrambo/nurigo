<query id="getBoardList" action="select">
	<tables>
		<table name="nstore_member_total" />
	</tables>
	<columns>
		<column name="*" />
	</columns>
	<conditions>
		<condition operation="equal" column="member_srl" var="member_srl" />
	</conditions>
	<navigation>
		<index var="sort_index" default="last_regdate" order="desc" />
		<list_count var="list_count" default="20" />
		<page_count var="page_count" default="10" />
		<page var="page" default="1" />
	</navigation>
</query>
