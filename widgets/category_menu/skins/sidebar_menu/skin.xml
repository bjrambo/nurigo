<?xml version="1.0" encoding="UTF-8"?>
<skin version="0.2">
    <title xml:lang="ko">Sunny</title>
    <description xml:lang="ko">
		컨텐츠몰 레이아웃 전용 카테고리메뉴 스킨입니다.
    </description>
    <version>1.0</version>
    <date>2012-10-17</date>
    <link>http://store.nurigo.net/</link>

    <author email_address="sunny@nurigo.net" link="http://www.nurigo.net/">
        <name xml:lang="ko">Sunny</name>
        <name xml:lang="en">Sunny</name>
    </author>

    <colorset>
		<color name="gray">
			<title xml:lang="ko">회 색(기본)</title>
			<title xml:lang="en">gray(default)</title>
		</color>
		<color name="green">
			<title xml:lang="ko">녹 색</title>
			<title xml:lang="en">green</title>
		</color>
		<color name="pink">
			<title xml:lang="ko">분홍색</title>
			<title xml:lang="en">pink</title>
		</color>
		<color name="purple">
			<title xml:lang="ko">보라색</title>
			<title xml:lang="en">purple</title>
		</color>
		<color name="blue">
			<title xml:lang="ko">파란색</title>
			<title xml:lang="en">blue</title>
		</color>
    </colorset>

	<extra_vars>
		<title xml:lang="ko">서브메뉴 사용여부</title>
			<var name="sub_menu" type="select">
				<title xml:lang="ko">서브메뉴선택</title>
				<title xml:lang="en">Sub Menu select</title>
				<description xml:lang="ko">서브메뉴 사용여부를 선택해 주세요.</description>
				<description xml:lang="en">Choose whether to use the Sub Menu.</description>
				 <options value="enabled">
					<title xml:lang="ko">사용함</title>
					<title xml:lang="en">Enabled</title>
				</options>
				 <options value="disabled">
					<title xml:lang="ko">사용안함</title>
					<title xml:lang="en">Disabled</title>
				</options>
			</var>
			<var name="sub2_menu" type="select">
				<title xml:lang="ko">드롭다운메뉴 선택</title>
				<title xml:lang="en">Dropdown_Menu select</title>
				<description xml:lang="ko">드롭다운메뉴 사용여부를 선택해 주세요.</description>
				<description xml:lang="en">Choose whether to use the Dropdown_Menu.</description>
				 <options value="enabled">
					<title xml:lang="ko">사용함</title>
					<title xml:lang="en">Enabled</title>
				</options>
				 <options value="disabled">
					<title xml:lang="ko">사용안함</title>
					<title xml:lang="en">Disabled</title>
				</options>
			</var>
	</extra_varas>
</skin>
